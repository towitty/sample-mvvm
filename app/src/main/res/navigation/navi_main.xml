<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/navigation_notify">

    <fragment
        android:id="@+id/navigation_calculator"
        android:name="com.yjkim.mvvmweather.view.fragment.CalculatorFragment"
        android:label="CalculatorFragment"
        tools:layout="@layout/fragment_calculator" />

    <fragment
        android:id="@+id/navigation_notify"
        android:name="com.yjkim.mvvmweather.view.fragment.NotifyFragment"
        android:label="ProductDetailFragment"
        tools:layout="@layout/fragment_notify" />

    <fragment
        android:id="@+id/navigation_edit"
        android:name="com.yjkim.mvvmweather.view.fragment.EditFragment"
        android:label="CategoryDetailFragment"
        tools:layout="@layout/fragment_edit" />
